{
  "version": 2,
  "builds": [
    {
      "src": "webhook-form/package.json",
      "use": "@vercel/next",
      "config": {
        "distDir": ".next"
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/webhook-form/$1"
    }
  ],
  "env": {
    "NEXT_PUBLIC_N8N_WEBHOOK_URL": "https://primary-production-1cd8.up.railway.app/webhook/webhook-phone-automation"
  }
}
